+import React, {Component} from 'react';
+
+class Todo extends Component {
+    render() {
+        let {item, selectedDeal} = this.props
+        return (
+            <div>
+                {item.id}-{item.title}
+                <button onClick={() => selectedDeal(item.id)}>Chose me</button>
+            </div>
+        );
+    }
+}
