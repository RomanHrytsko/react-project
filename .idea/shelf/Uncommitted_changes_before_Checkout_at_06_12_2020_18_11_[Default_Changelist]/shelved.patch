@@ -0,0 +1,47 @@
+import React, {Component} from 'react';
+import Todo from "./Todo";
+import './todoStyles.css'
+
+class AllTodos extends Component {
+    state = {todo: [], classState: 'one', selectedDeal: null}
+    flag = false
+
+    componentDidMount() {
+        fetch('https://jsonplaceholder.typicode.com/todos')
+            .then(value => value.json())
+            .then(todoFromApi => this.setState({todo: todoFromApi}))
+    }
+
+    changecolor = () => {
+        if (this.flag) {
+            this.setState({classState: 'one'})
+        } else {
+            this.setState({classState: 'two'})
+        }
+        this.flag = !this.flag
+    }
+    selectTodo = (id) => {
+        let chosen = this.state.todo.find(value => value.id === id);
+        this.setState({selectedDeal: chosen})
+    }
+
+
+    render() {
+        let {todo, classState, selectedDeal} = this.state
+        return (
+            <div>
+                <h1 onClick={this.changecolor} className={classState}>To do list</h1>
+                {
+                    todo.map(value => (<Todo item={value} key={value.id} selectedDeal={this.selectTodo}/>))
+                }
+
+                <hr/>
+                {
+                    selectedDeal && <Todo item={selectedDeal}/>
+                }
+            </div>
+        );
+    }
+}
+
+export default AllTodos;
